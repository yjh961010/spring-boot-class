<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.student.student-mapper">
	<select id ="studentlist" resultType="StudentDTO">
	select *from student
	</select>
<select id="findStudent" parameterType="String" resultType="StudentDTO"> select * from student where cname = #{cname} </select>
<insert id="insertStudent" parameterType="StudentDTO"> insert into student values(#{id}, #{name}, #{cname}) </insert>
<delete id="deleteStudent" parameterType="String"> delete from student where id = #{id} </delete>
</mapper>